{
    "name": "SDM230-M",
    "category": "Energy meters",
    "group": "B+G E-TECH",
    "description": "<b>Energy Meter SDM230-M</b>:<br>Current: 10(100)A<br>Voltage : 230V<br>50(60)Hz",
    
    "prefix": "name",
    
    "devices": [
        {
            "driver": "modbus",
            "description": "Energy Meter",
            "settings": "RTU:SERIAL_ENCODING_RTU:9600:DATABITS_8:PARITY_NONE:STOPBITS_2:ECHO_FALSE:FLOWCONTROL_NONE:FLOWCONTROL_NONE"
        }
    ],
    
    "options": [
        {
            "id": "address",
            "name": "Address",
            "description": "The physical device address to uniquely identify a physical smart meter.<br><br><b>Example:</b><ol><li><b>Linux</b>: /dev/ttyS0 or /dev/ttyUSB0</li><li><b>Windows</b>: COM1</li></ol>",
            "type": "text",
            "mandatory": true,
            "syntax": "deviceAddress"
        },
        {
            "id": "unitId",
            "name": "Unid ID",
            "description": "The device address of the Modbus slave.<br>The slave device address is assigned form 1 to 247.<br>Address 0 is used as broadcast address.</li></ul>",
            "type": "text",
            "mandatory": true,
            "syntax": "channelAddress"
        }
    ],
    
    "channels": [
        {
            "name": "voltage",
            "description": "Voltage",
            "address": "<unitId>:INPUT_REGISTERS:0:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "V", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "voltage"}
                }
            ]
        },
        {
            "name": "power",
            "description": "Power",
            "address": "<unitId>:INPUT_REGISTERS:12:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "W", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "power"}
                }
            ]
        },
        {
            "name": "frequency",
            "description": "Frequency",
            "address": "<unitId>:INPUT_REGISTERS:70:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "Hz", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "frequency"}
                }
            ]
        },
        {
            "name": "import_energy",
            "description": "Import energy",
            "address": "<unitId>:INPUT_REGISTERS:72:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "kWh", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "import_energy"}
                }
            ]
        },
        {
            "name": "export_energy",
            "description": "Export energy",
            "address": "<unitId>:INPUT_REGISTERS:74:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "kWh", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "export_energy"}
                }
            ]
        },
        {
            "name": "total_system_power",
            "description": "Total system power demand",
            "address": "<unitId>:INPUT_REGISTERS:84:FLOAT",
            "logging": {"loggingInterval": 30000, "loggingMaxInterval": 180000},
            "configs": {"samplingInterval": 30000, "unit": "W", "valueType":"FLOAT"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "total_system_power"}
                }
            ]
        }
    ],
    
    "feeds": [
        {
            "name": "voltage",
            "tag": "Voltage",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "power",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "frequency",
            "tag": "Frequency",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "import_power",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "export_power",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "total_system_power",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        }
    ],
    
    "syntax": {
        "deviceAddress": {
            "keyValue": false,
            "separator": ":"
        }
    }
}